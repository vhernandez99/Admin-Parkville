<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Admin_Parkville.Views.Notificaciones">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Titulo" x:Name="TituloEntry"></Entry>
            <Entry Placeholder="Cuerpo" x:Name="CuerpoEntry"></Entry>
            <Button Text="Enviar" x:Name="Enviar" Clicked="Enviar_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>